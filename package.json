{
  "name": "node-ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts | npx pino-pretty",
    "build": "tsc",
    "start": "node dist/index.js",
    "docker": "docker compose -f docker-compose.yaml up",
    "database": "docker compose exec db psql -U postgres",
    "app": "docker compose exec app /bin/sh",
    "migrate": "node-pg-migrate",
    "migrate:create": "pnpm migrate create -j ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:docker": "docker compose exec app pnpm test",
    "test:ci": "docker compose run --rm app pnpm test",
    "reset": "docker compose down -v && docker compose up --build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.1",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "node-pg-migrate": "^8.0.4",
    "pino-pretty": "^13.1.3",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.18.0",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "zod": "^4.3.6"
  }
}
